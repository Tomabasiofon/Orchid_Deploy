        <script>
            document.addEventListener("DOMContentLoaded", function(event){

                const retrieveForm = document.getElementById('retrieve-booking-form');
                const retieveInput = document.getElementById('tx-ref-input');

                retrieveForm.addEventListener('submit', (e) =>{
                    e.preventDefault();
                    console.log(retieveInput.value.length);
                    const tx_ref = retieveInput.value;

                    if(tx_ref.length > 7) {
                        alert('Please eneter a valid Invoice id')
                    } else {
                        window.location.href = `https://orchidsprings.cyclic.cloud/confirmation?tx_ref=${tx_ref}&status=successful`;
                    }

                })

                const clients = document.getElementById('happy-client')
                const options = {
                    url: `https://orchidsprings.cyclic.cloud/api/people`,
                    method: 'GET',
                    headers: {
                    'Accept': 'application/json',
                    'Content-Type': 'application/json;charset=UTF-8'
                    },
                };
                axios(options)
                    .then(response => {
                        clients.setAttribute('data-count', response.data.length)
                    });

            });


        </script>
